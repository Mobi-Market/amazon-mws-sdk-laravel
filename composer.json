{
    "name": "mobi-market/amazon-sdk-laravel",
    "type": "library",
    "description": "Amazon MWS SDK Laravel Package",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "illuminate/support": "~5.5.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Callum Slaney-Flett",
            "email": "callum@mobi-market.co.uk"
        },
        {
            "name": "Scott-David Jones",
            "email": "scott.jones@autumndev.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MobiMarket\\Amazon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MobiMarket\\Amazon\\AmazonServiceProvider"
            ],
            "aliases": {
                "AmazonEasyShip": "MobiMarket\\Amazon\\Facades\\EasyShipFacade",
                "AmazonFeeds": "MobiMarket\\Amazon\\Facades\\FeedsFacade",
                "AmazonFinances": "MobiMarket\\Amazon\\Facades\\FinancesFacade",
                "AmazonFulfillmentInbound": "MobiMarket\\Amazon\\Facades\\FulfillmentInboundFacade",
                "AmazonFulfillmentInventory": "MobiMarket\\Amazon\\Facades\\FulfillmentInventoryFacade",
                "AmazonFulfillmentOutbound": "MobiMarket\\Amazon\\Facades\\FulfillmentOutboundFacade",
                "AmazonMerchantFulfillment": "MobiMarket\\Amazon\\Facades\\MerchantFulfillmentFacade",
                "AmazonOrders": "MobiMarket\\Amazon\\Facades\\OrdersFacade",
                "AmazonProducts": "MobiMarket\\Amazon\\Facades\\ProductsFacade",
                "AmazonRecommedations": "MobiMarket\\Amazon\\Facades\\RecommedationsFacade",
                "AmazonReports": "MobiMarket\\Amazon\\Facades\\ReportsFacade",
                "AmazonSellers": "MobiMarket\\Amazon\\Facades\\SellersFacade",
                "AmazonSubscriptions": "MobiMarket\\Amazon\\Facades\\SubscriptionsFacade"
            }
        }
    }
}
